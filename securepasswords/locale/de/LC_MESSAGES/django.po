#: admin.py:35
msgid "Force selected users to change their passwords"
msgstr "Ausgewählte user zwingen, ihr Passwort zu ändern"

#: apps.py:7
msgid "secure passwords"
msgstr "Passwortsicherheit"

#: middleware.py:24
msgid "You must change your password for security reasons!"
msgstr "Sie müssen aus Sicherheitsgründen Ihr Passwort ändern!"

#: middleware.py:34
#, python-brace-format
msgid ""
"Your current password is older than {max_age} days. Please change your "
"password now."
msgstr ""
"Ihr aktuelles Passwort ist älter als {max_age}. Bitte ändern Sie jetzt "
"Ihr Passwort."

#: models.py:11
msgid "force change"
msgstr "Änderung erzwingen"

#: models.py:14
msgid "password profile"
msgstr "Passwortprofil"

#: models.py:15
msgid "password profiles"
msgstr "Passwortprofile"

#: models.py:35 models.py:39
msgid "password"
msgstr "Passwort"

#: models.py:40
msgid "passwords"
msgstr "Passwörter"

#: validators.py:20
msgid "You have used this password before."
msgstr "Sie haben dieses Passwort in der Vergangenheit bereits verwendet."

#: validators.py:25
msgid "You must not reuse a previous password."
msgstr "Sie dürfen kein Passwort wiederverwenden."

#: validators.py:35
#, python-brace-format
msgid "Your password contains more than {n} consecutive equal characters."
msgstr "Ihr Passwort enthält mehr als {n} aufeinanderfolgende gleiche Zeichen."

#: validators.py:40
#, python-brace-format
msgid ""
"Your password must not contain more than {n} consecutive equal characters."
msgstr ""
"Ihr Passwort darf nicht mehr als {n} aufeinanderfolgende gleiche Zeichen enthalten."

#: validators.py:52
#, python-brace-format
msgid ""
"Your password contains an arithmetic sequence ('456', '642', etc.) longer than {n}"
msgstr ""
"Ihr Passwort enthält eine arithmetische Sequenz ('456', '642', etc.), die länger als "
"{n} Zeichen ist."

#: validators.py:57
#, python-brace-format
msgid ""
"Your password must not contain an arithmetic sequence ('456', '642', etc.) "
"longer than {n}"
msgstr ""
"Ihr Passwort darf keine eine arithmetische Sequenz ('456', '642', etc.) "
"enthalten, die länger als {n} Zeichen ist."
