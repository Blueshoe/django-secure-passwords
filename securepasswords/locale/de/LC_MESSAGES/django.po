#: securepasswords/admin.py:35
msgid "Force selected users to change their passwords"
msgstr "Ausgewählte Nutzer zwingen, ihre Passwörter zu ändern"

#: securepasswords/apps.py:7
msgid "secure passwords"
msgstr "sichere Passwörter"

#: securepasswords/middleware.py:24
msgid "You must change your password for security reasons!"
msgstr "Sie müssen aus Sicherheitsgründen Ihr Passwort ändern"

#: securepasswords/middleware.py:34
#, python-brace-format
msgid ""
"Your current password is older than {max_age} days. Please change your "
"password now."
msgstr ""
"Ihr aktuelles Passwort ist älter als {max_age} Tage. Bitte ändern Sie "
"jetzt Ihr Passwort."

#: securepasswords/models.py:11
msgid "force change"
msgstr "Änderung erzwingen"

#: securepasswords/models.py:14
msgid "password profile"
msgstr "Passwortprofil"

#: securepasswords/models.py:15
msgid "password profiles"
msgstr "Passwortprofile"

#: securepasswords/models.py:35 securepasswords/models.py:39
msgid "password"
msgstr "Passwort"

#: securepasswords/models.py:40
msgid "passwords"
msgstr "Passwörter"

#: securepasswords/validators.py:21
msgid "You have used this password before."
msgstr "Sie haben dieses Passwort bereits einmal verwendet."

#: securepasswords/validators.py:26
msgid "You must not reuse a previous password."
msgstr "Sie dürfen Passwörter nicht wiederverwenden."

#: securepasswords/validators.py:36
#, python-brace-format
msgid "Your password contains more than {n} consecutive equal characters."
msgstr "Ihr Passwort enthält mehr als {n} gleiche aufeinanderfolgende Zeichen."

#: securepasswords/validators.py:41
#, python-brace-format
msgid ""
"Your password must not contain more than {n} consecutive equal characters."
msgstr ""
"Ihr Passwort darf nicht mehr als {n} gleiche aufeinanderfolgende Zeichen enthalten."

#: securepasswords/validators.py:79
#, python-brace-format
msgid ""
"Your password must not contain an arithmetic sequence ('456', '642', etc.) "
"longer than {n}"
msgstr ""
"Ihr Passwort darf keine arithmetische Sequenz ('456', '642', etc.) enthalten, "
"die länger als {n} ist."

#: securepasswords/validators.py:84
#, python-brace-format
msgid ""
"Your password contains an arithmetic sequence ('456', '642', etc.) longer "
"than {n}"
msgstr ""
"Ihr Passwort enthält eine arithmetische Sequenz ('456', '642', etc.), "
"die länger als {n} ist."

#: securepasswords/validators.py:98
#, python-brace-format
msgid ""
"Your password must not contain an alphabetic sequence ('dcba', 'aceg', etc.) "
"longer than {n}"
msgstr ""
"Ihr Passwort darf keine alphabetische Abfolge ('dcba', 'aceg', etc.) enthalten, "
"die länger als {n} ist."

#: securepasswords/validators.py:103
#, python-brace-format
msgid ""
"Your password contains an alphabetic sequence ('dcba', 'aceg', etc.) longer "
"than {n}"
msgstr ""
"Ihr Passwort enthält eine alphabetische Abfolge ('dcba', 'aceg', etc.), "
"die länger als {n} ist."

#: securepasswords/validators.py:125
#, python-brace-format
msgid ""
"Your password must contain characters from at least {n} categories out of "
"the following 5: Uppercase letters, lowercase letters, digits, punctuation, "
"whitespace"
msgstr ""
"Ihr Passwort muss Zeichen aus mindestens {n} der folgenden 5 Klassen enthalten: "
"Großbuchstaben, Kleinbuchstaben, Ziffern, Sonderzeichen, Leerzeichen"

#: securepasswords/validators.py:134
#, python-brace-format
msgid ""
"Your password contains characters from fewer than {n} categories out of the "
"following 5: Uppercase letters, lowercase letters, digits, punctuation, "
"whitespace"
msgstr ""
"Ihr Passwort enthält Zeichen aus weniger als {n} der folgenden 5 Klassen: "
"Großbuchstaben, Kleinbuchstaben, Ziffern, Sonderzeichen, Leerzeichen"
